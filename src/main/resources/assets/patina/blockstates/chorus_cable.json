{
  "multipart": [
    {"apply": {"model": "patina:block/chorus_cable_center"}},

    {"when": {"north": "true"}, "apply": {"model": "patina:block/chorus_cable"}},

    {"when": {"east": "true"}, "apply": {"model": "patina:block/chorus_cable", "y": 90}},

    {"when": {"south": "true"}, "apply": {"model": "patina:block/chorus_cable", "y":  180}},

    {"when": {"west": "true"}, "apply": {"model": "patina:block/chorus_cable","y": 270}},

    {"when": {"up": "true"}, "apply": {"model": "patina:block/chorus_cable","x": 270}},

    {"when": {"down": "true"}, "apply": {"model": "patina:block/chorus_cable","x": 90}}
  ]
}